{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-airbnb"
	],
	"jsRules": {},
	"rules": {
		"indent": [true, "tabs", 2],
		"ter-indent": [false],
		"semicolon": [true, "never"],
		"no-console": [false],
		"object-literal-sort-keys": [false],
		"variable-name": [false],
		"max-classes-per-file": [true, 2]
	},
	"rulesDirectory": []
}